#include <iostream>
using namespace std;

voidbubbleSort(intarr[], int n) {
for (inti = 0; i < n - 1; i++)
for (int j = 0; j < n - i - 1; j++) {
if (arr[j] >arr[j + 1]) {
int temp = arr[j];
arr[j] = arr[j + 1];
arr[j + 1] = temp;
            }
        }
    }
}

int main() {
intarr[] = {64, 34, 25, 12, 22, 11, 90};
int n = sizeof(arr) / sizeof(arr[0]);

cout<< "Original array: ";
for (inti = 0; i< n; i++)
cout<<arr[i] << " ";
cout<<endl;

bubbleSort(arr, n);

cout<< "Sorted array: ";
for (inti = 0; i< n; i++)
cout<<arr[i] << " ";
cout<<endl;

return 0;
}
