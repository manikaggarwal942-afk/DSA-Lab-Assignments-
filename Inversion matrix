#include <iostream>
using namespace std;

intcountInversionsNaive(intarr[], int n) {
intinvCount = 0;
for (inti = 0; i< n - 1; i++) {
for (int j = i + 1; j < n; j++) {
if (arr[i] >arr[j]) {
invCount++;
            }
        }
    }
returninvCount;
}

int main() {
intarr[] = {2, 4, 1, 3, 5};
int n = sizeof(arr) / sizeof(arr[0]);

cout<< "Number of inversions (Naive): " <<countInversionsNaive(arr, n) <<endl;
return 0;
}
