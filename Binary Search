#include <iostream>
using namespace std;

intbinarySearch(intarr[], int n, int key) {
int low = 0, high = n - 1;

while (low <= high) {
int mid = (low + high) / 2;

if (arr[mid] == key) {
return mid; 
        } 
else if (arr[mid] < key) {
low = mid + 1;        } 
else {
high = mid - 1; 
        }
    }

return -1; 
}

int main() {
int n, key;

cout<< "Enter number of elements: ";
cin>> n;

intarr[n];
cout<< "Enter elements in sorted order: ";
for (inti = 0; i< n; i++) {
cin>>arr[i];
    }

cout<< "Enter element to search: ";
cin>> key;

int result = binarySearch(arr, n, key);

if (result != -1) {
cout<< "Element found at index " << result <<endl;
    } else {
cout<< "Element not found in the array." <<endl;
    }

return 0;
}
