#include <iostream>
#include <queue>
using namespace std;

class StackUsingTwoQueues {
private:
    queue<int> queue1, queue2;

public:
    void push(int x) {
        queue1.push(x);
    }

    int pop() {
        if (queue1.empty()) {
            cout << "Stack is empty!" << endl;
            return -1;
        }

        while (queue1.size() > 1) {
            queue2.push(queue1.front());
            queue1.pop();
        }

        int top = queue1.front();
        queue1.pop();
        swap(queue1, queue2);
        return top;
    }

    int top() {
        if (queue1.empty()) {
            cout << "Stack is empty!" << endl;
            return -1;
        }

        while (queue1.size() > 1) {
            queue2.push(queue1.front());
            queue1.pop();
        }

        int top = queue1.front();
        queue2.push(top);
        queue1.pop();
        swap(queue1, queue2);
        return top;
    }

    bool isEmpty() {
        return queue1.empty();
    }
};

int main() {
    StackUsingTwoQueues stack;

    stack.push(10);
    stack.push(20);
    stack.push(30);
    stack.push(40);

    cout << "Top of stack: " << stack.top() << endl;
    cout << "Pop from stack: " << stack.pop() << endl;
    cout << "Pop from stack: " << stack.pop() << endl;
    cout << "Top of stack: " << stack.top() << endl;

    stack.push(50);
    cout << "Pop from stack: " << stack.pop() << endl;
    cout << "Pop from stack: " << stack.pop() << endl;
    cout << "Pop from stack: " << stack.pop() << endl;

    if (stack.isEmpty()) {
        cout << "Stack is empty!" << endl;
    }

    return 0;
}
